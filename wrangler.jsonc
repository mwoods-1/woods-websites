{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "woods-websites",
  // Personal Cloudflare account (woods04sean@gmail.com) â€” woodswebsites.com domain is here
  "account_id": "5d903511ec412735b20423c7157ab2ae",
  // Entry point: the Worker bundle produced by `opennextjs-cloudflare build`
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-12-01",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  // Static assets served directly by the CDN; Worker handles dynamic routes
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  // Self-reference service binding required for on-demand revalidation
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "woods-websites"
    }
  ]
}
